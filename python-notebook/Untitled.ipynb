    "def get_initials(fullname):\n",
    "    fullname = input('What is your full name?')\n",
    "    name = fullname.split()    \n",
    "    result = ''\n",
    "    \n",
    "    for n in name:\n",
    "        result += n[0].upper()\n",
    "        \n",
    "    return result\n",
    "    "
